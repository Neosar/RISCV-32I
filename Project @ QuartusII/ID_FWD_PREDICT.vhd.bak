LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_MISC.ALL;

USE WORK.TOOLBOX.ALL;

ENTITY ID_FWD_PREDICT IS

	PORT (
			RS1 : IN  STD_LOGIC_VECTOR(31 DOWNTO 0);
			RS2 : IN  STD_LOGIC_VECTOR(31 DOWNTO 0);
			RD  : IN  STD_LOGIC_VECTOR(31 DOWNTO 0);
			
			IMGEN : IN STD_LOGIC_VECTOR(2 DOWNTO 0); -- Used to detect U/R - Commands
			MEMOP : IN STD_LOGIC_VECTOR(2 DOWNTO 0); -- Used to detect Stores
			JUMP  : IN STD_LOGIC; -- Used to detect Jump
			JALR  : IN STD_LOGIC; -- /
			BRANCH: IN STD_LOGIC; -- Used to detect Branches
			
			FWDA : OUT STD_LOGIC;
			FWDB : OUT STD_LOGIC;
			FWDC : OUT STD_LOGIC
		 );
		 
END ID_FWD_PREDICT;

